{
    "name": "MagicBeans Django+PostgreSQL",
    "dockerComposeFile": "../docker-compose.local.yml",
    "service": "app",
    "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}",
    "forwardPorts": [8000],
    "customizations": {
        "vscode": {
            "extensions": [
                "ms-python.python",
                "ms-python.vscode-pylance",
                "visualstudioexptteam.vscodeintellicode",
                "eamodio.gitlens"
            ]
        }
    },
    "remoteUser": "root"
}
